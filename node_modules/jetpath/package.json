{
  "name": "jetpath",
  "version": "1.5.21",
  "description": "JetPath - A fast, seamless and minimalist framework for Node, Deno and Bun.js. Embrace the speed and elegance of the next-gen server-side experience.",
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist/index.d.ts",
    "dist/primitives",
    "dist/index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fridaycandour/jetpath.git"
  },
  "keywords": [
    "web",
    "framework",
    "fast",
    "simple",
    "bunjs",
    "nodejs",
    "denojs",
    "expressive",
    "server",
    "http"
  ],
  "author": "fridaycandour fridaycandours@gmail.com",
  "license": "Apache",
  "bugs": {
    "url": "https://github.com/fridaycandour/jetpath/issues"
  },
  "homepage": "https://github.com/fridaycandour/jetpath#readme",
  "scripts": {
    "compile": "bun bundle.ts",
    "node": "node tests/app.js",
    "deno": "deno run --allow-all tests/app.jet.js",
    "bun": "bun --watch tests/app.jet.js",
    "dev": "nodemon tests/app.jet.js",
    "watch": "tsc tests/*.ts --target esnext --watch",
    "build": "bun bundle.ts"
  },
  "engines": {
    "node": ">=14.0.0",
    "bun": ">=0.1.0"
  },
  "private": false,
  "devDependencies": {
    "@types/bun": "^1.1.8",
    "@types/busboy": "^1.5.4",
    "@types/node": "^20.11.16",
    "busboy": "^1.6.0",
    "ws": "^8.17.0"
  },
  "dependencies": {
    "mime": "^4.0.1"
  }
}
